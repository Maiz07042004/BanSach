									<div class="text-accordion">
										<div class="tag_cloud_wrapper">
											<ul>
												@foreach(var itemDanhMuc in Model.Categories)
												{
													<li class=@((itemDanhMuc.CategoryId==Model.Book.Category.CategoryId)?"active":"")>
														<a href="#" >@itemDanhMuc.CategoryName</a>
													</li>
												}
												
											</ul>
										</div>
									</div>
								</div>
								<!-- end of panel-body -->
							</div>
						</div>
